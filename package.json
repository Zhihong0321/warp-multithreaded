{
  "name": "warp-multithreaded",
  "version": "1.0.0",
  "description": "Enable multithreaded AI agent development with coordinated parallel sessions in Warp Terminal",
  "main": "core/session-manager.js",
  "bin": {
    "warp-multithreaded": "./scripts/coordinator.js"
  },
  "scripts": {
    "test": "node test/run-tests.js",
    "validate": "node scripts/validate-system.js",
    "health-check": "node scripts/health-check.js",
    "start": "node scripts/coordinator.js",
    "init": "node scripts/coordinator.js init",
    "status": "node scripts/coordinator.js status",
    "demo": "node examples/demo.js",
    "dashboard": "node dashboard/server.js",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup-windows.ps1",
    "setup:unix": "bash scripts/setup-unix.sh",
    "postinstall": "node scripts/health-check.js"
  },
  "keywords": [
    "warp",
    "multithreaded",
    "ai-agents",
    "terminal",
    "parallel-development",
