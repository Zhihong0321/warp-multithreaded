{
  "name": "warp-multithreaded",
  "version": "1.3.0",
  "description": "Enable multithreaded AI agent development with coordinated parallel sessions in Warp Terminal",
  "main": "core/session-manager.js",
  "bin": {
    "warp-multithreaded": "./scripts/coordinator.js",
    "wm": "./wm.js"
  },
  "scripts": {
    "test": "node test/run-tests.js",
    "validate": "node scripts/validate-system.js",
    "health-check": "node scripts/health-check.js",
    "validate-comprehensive": "node scripts/comprehensive-validation.js",
    "start": "node scripts/coordinator.js",
    "init": "node scripts/coordinator.js init",
    "status": "node scripts/coordinator.js status",
    "demo": "node examples/demo.js",
    "dashboard": "node dashboard/server.js",
    "cli-service": "node scripts/dashboard-cli-service.js",
    "masterplan": "node scripts/generate-unified-rules.js",
    "masterplan-wizard": "node scripts/masterplan-wizard.js",
    "goal": "node scripts/goal-manager.js",
    "setup:windows": "powershell -ExecutionPolicy Bypass -File scripts/setup-windows.ps1",
    "setup:unix": "bash scripts/setup-unix.sh",
    "rules:check": "node scripts/rules-checker.js --status",
    "rules:setup": "node scripts/rules-checker.js --setup",
    "rules:verify": "node scripts/rules-checker.js --verify",
    "update-state": "node scripts/coordinator.js update-state",
    "load-state": "node scripts/coordinator.js load-state",
    "save-state": "node scripts/coordinator.js update-state",
    "startup": "node scripts/coordinator.js startup",
    "shutdown": "node scripts/coordinator.js shutdown",
    "tasks": "node scripts/coordinator.js masterplan tasks",
    "context": "node scripts/coordinator.js context",
    "postinstall": "node scripts/health-check.js"
  },
  "keywords": [
    "warp",
    "multithreaded",
    "ai-agents",
    "terminal",
    "parallel-development",
    "productivity",
    "coordination",
    "multi-session",
    "development-tools"
  ],
  "author": "Your Name",
  "license": "MIT",
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.26.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.7.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/warp-multithreaded.git"
  },
  "bugs": {
    "url": "https://github.com/yourusername/warp-multithreaded/issues"
  },
  "homepage": "https://github.com/yourusername/warp-multithreaded#readme"
}
